{
  "if operator": {
    "prefix": "if",
    "description": "if operator",
    "body": "if ${1:condition}\n    ${2}\nend"
  },
  "if else operator": {
    "prefix": "if else",
    "description": "if else operator",
    "body": "if ${1:condition}\n    ${2}\nelse\n    ${3}\nend"
  },
  "for operator": {
    "prefix": "for",
    "description": "for operator",
    "body": "for ${1:item} in ${2:(seq 1 10)}\n    ${3}\nend"
  },
  "while operator": {
    "prefix": "while",
    "description": "while operator",
    "body": "while ${1:condition}\n    ${2}\nend"
  },
  "switch operator": {
    "prefix": "switch",
    "description": "Create switch operator",
    "body": "switch ${1:\\$variable}\n    case ${2:1}\n    ${3}\n    case '*'\n    ${4}\nend"
  },
  "function definition": {
    "prefix": "function",
    "description": "function definition",
    "body": "function ${1:name}\n    ${3}\nend"
  },
  "begin end block": {
    "prefix": "begin",
    "description": "begin end block",
    "body": "begin\n    ${1}\nend"
  },
  "and operator": {
    "prefix": "and",
    "description": "and operator",
    "body": "${1:true}; and ${2:true}"
  },
  "or operator": {
    "prefix": "or",
    "description": "or operator",
    "body": "${1:true}; or ${2:true}"
  },
  "set builtin call": {
    "prefix": "set",
    "description": "'set' invocation",
    "body": "set ${1|-l,--local,-x,--export,-g,--global,-U,--universal,-u,--unexport,--path,--unpath,-f,--function|} ${2:variable} ${3:value}"
  },
  "string builtin call": {
    "prefix": "string",
    "description": "'string' invocation",
    "body": "string ${1|collect,escape,join,join0,length,lower,match,pad,repeat,replace,split,split0,sub,trim,unescape,upper|} ${2:\\$variable}"
  },
  "test builtin call": {
    "prefix": "test",
    "description": "'test' invocation",
    "body": "test ${1:\\$value1} ${2|==,!=,-eq,-ne,-gt,-ge,-lt,-le|} ${3:\\$value2}"
  },
  "comment": {
    "prefix": "#",
    "description": "comment",
    "body": "# ${1}"
  }
}